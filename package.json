{
  "name": "website-landing-page",
  "version": "1.0.0",
  "description": "Website landing page using tailwindcss and postcss",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss src/styles/styles-input.css -o src/styles/styles-output.css -w src/{*}.html",
    "server": "live-server ./src",
    "dev": "concurrently --kill-others \"npm run build:css\" \"npm run server\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "live-server": "^1.2.2",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-reporter": "^7.0.5",
    "tailwindcss": "^3.4.0"
  }
}
